         <font>
            <Font size="23.0" />
         </font>
      </Label>
      <TextField fx:id="requestusername" layoutX="189.0" layoutY="144.0" prefHeight="34.0" prefWidth="235.0" promptText="USERNAME" />
      <PasswordField fx:id="requestpassword" layoutX="189.0" layoutY="217.0" prefHeight="34.0" prefWidth="235.0" promptText="PASSWORD" />
      <Button fx:id="ok" layoutX="272.0" layoutY="283.0" mnemonicParsing="false" onAction="#Ok" prefHeight="25.0" prefWidth="69.0" text="ok" />
      <Button fx:id="log" layoutX="474.0" layoutY="330.0" mnemonicParsing="false" onAction="#login" text="LOGIN" />
   </children>
</AnchorPane>
